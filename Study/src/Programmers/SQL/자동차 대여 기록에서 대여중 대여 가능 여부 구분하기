-- 코드를 입력하세요
SELECT CAR_ID,
    CASE
        WHEN CAR_ID IN (
            SELECT CAR_ID
            FROM CAR_RENTAL_COMPANY_RENTAL_HISTORY
            WHERE START_DATE <= '2022-10-16' AND END_DATE >= '2022-10-16'
        ) THEN "대여중"
        ELSE "대여 가능"
    END AS "AVAILABILITY"
FROM CAR_RENTAL_COMPANY_RENTAL_HISTORY CH
GROUP BY CAR_ID
ORDER BY CAR_ID DESC;

# CASE WHEN THEN 절 안에 서브쿼리를 집어넣을 수 있다!!